
@{
    ViewData["Title"] = "Page";
}

<h1>Page</h1>


@if (ViewBag.err)
{
    <h4 class="alert alert-danger">@ViewBag.msg</h4>
}
else
{
    <h4 class="alert alert-primary">News Article Details</h4>
    <br />
    <h2 class="alert alert-success">@ViewBag.date</h2>
    <br />
    <h2 class="alert alert-warning">@ViewBag.page</h2>
    <br />
    <br />
}
